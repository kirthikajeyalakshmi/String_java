// User function Template for Java

class Solution {
    int isSame(String s) {
        // code here
        int num=0;
        int count=0;
        for(char ch:s.toCharArray()){
            if(ch>='0' && ch<='9'){
                num=num*10 + (ch-'0');
                count++;
            }
        }
        if(num==s.length()-count){
            return 1;
        }else{
            return 0;
        }
    }
}
