// User function Template for Java
class Solution {
    String isGoodString(String s) {
        // code here
        for(int i=0;i<s.length()-1;i++){
            if(Math.abs(s.charAt(i) - s.charAt(i+1))!=1 &&
            Math.abs(s.charAt(i) - s.charAt(i+1))!=25
            ){
                return "NO";
            }
        }
        return "YES";
    }
}
