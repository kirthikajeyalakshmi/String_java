// User function Template for Java
class Solution {
    char firstRep(String S) {
        // your code here
        for(int i=0;i<S.length();i++){
            char ch=S.charAt(i);
            for(int j=i+1;j<S.length();j++){
                if(ch==S.charAt(j)){
                    return ch;
                }
            }
        }
        return '#';
    }
}
